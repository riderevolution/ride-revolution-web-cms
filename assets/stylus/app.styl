.cms_taxonomy
    display: flex
    flex-flow: row wrap
    .taxonomy_wrapper
        padding: 0 20px 40px
        flex: 0 0 50%
    .taxonomy_body
        background-color: $white
        box-shadow: 15px 25px 40px transparent
        transition: .3s ease-in-out
        &:hover
            box-shadow: 15px 25px 40px rgba(0, 0, 0, 0.05)
            transition: .3s ease-in-out
        .taxonomy_header
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            padding: 20px 20px 20px 35px
            border-bottom: 1px solid $border
            .header_title
                sf_pro_bold(22px)
                color: $black
        .taxonomy_content
            padding: 20px
            height: 100%
            max-height: 325px
            overflow-y: auto
        .taxonomy_body
            padding: 20px 0
            height: 100%
            max-height: 500px
            overflow-y: auto
        .taxonomy_footer
            padding: 20px 35px
    .taxonomy_actions
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: flex-end
        .action_btn
            width: auto
            display: flex
            flex-flow: row wrap
            justify-content: center
            align-items: center
            background-color: transparent
            border: 1px solid transparent
            svg
                margin-right: 10px
                .add
                    fill: $blue
                    transition: .3s ease-in-out
                .add_sign
                    fill: transparent
                    stroke: $white
                    transition: .3s ease-in-out
                .edit
                    fill: transparent
                    stroke: $blue
                    stroke-miterlimit: 10
                    stroke-width: 1.5
                    transition: .3s ease-in-out
                .edit_sign
                    fill: transparent
                    stroke: $blue
                    stroke-miterlimit: 10
                    stroke-width: 1.5
                    transition: .3s ease-in-out
            &:hover
                background-color: $blue
                color: $white
            &:focus
                background-color: $blue_alt
                color: $white
            &:focus
                .add_sign
                    fill: transparent
                    stroke: $blue_alt
                    transition: .3s ease-in-out
                .edit
                    fill: $blue_alt
                    stroke: $white
                    transition: .3s ease-in-out
            &:hover
                .add
                    fill: $white
                    transition: .3s ease-in-out
                .add_sign
                    fill: transparent
                    stroke: $blue
                    transition: .3s ease-in-out
                .edit
                    fill: $blue
                    stroke: $white
                    transition: .3s ease-in-out
                .edit_sign
                    stroke: $white
                    transition: .3s ease-in-out

.cms_autocomplete
    position: absolute
    top: 105%
    z-index: 2
    background-color: $white
    width: 100%
    padding: 10px 0
    border-radius: 3px
    box-shadow: 20px 20px 40px rgba(0, 0, 0, 0.16)
    &.scrollable
        height: 300px
        overflow-y: auto
    .autocomplete_title
        padding: 10px 20px
        sf_pro_medium(15px)
        color: $black
        background-color: transparent
        transition: .3s ease-in-out
        &:hover
            color: $green
            background-color: $light_green
            transition: .3s ease-in-out
.cms_summary
    display: block
    background-color: $white
    border-radius: 5px
    text-decoration: none
    margin-bottom: 60px
    box-shadow: 15px 25px 40px transparent
    animation: slide_in 1s
    transition: .3s ease-in-out
    .summary_header
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: space-between
        padding: 20px 30px
        border-bottom: 1px solid $border
        .header_title
            sf_pro_medium(25px)
            color: $black
    .summary_body
        display: flex
        flex-flow: row wrap
        align-items: center
        .column
            flex: 1 0 50%
            padding: 40px
            .total_label
                sf_pro_regular(15px)
                color: $black
                padding-bottom: 20px
            .total
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: space-between
                padding-top: 10px
                .total_label
                    flex: 1 0 40%
                    padding-left: 20px
                    padding-bottom: 0
                .total_count
                    sf_pro_semibold(20px)
                    color: $black
            &:first-child
                border-right: 1px solid $border
    &:hover
        box-shadow: 15px 25px 40px rgba(0, 0, 0, 0.05)
        transition: .3s ease-in-out

.wrapper
    .total_image
        position: relative
        .front
            opacity: 1
            transform: scaleX(1)
            transition: .3s ease-in-out
        .back
            position: absolute
            top: 0
            left: 0
            z-index: 1
            opacity: 0
            transform: scaleX(-1)
            transition: .3s ease-in-out
    &:hover
        .total_image
            .front
                opacity: 0
                transform: scaleX(-1)
                transition: .3s ease-in-out
            .back
                opacity: 1
                transform: scaleX(1)
                transition: .3s ease-in-out
/**
 * Default Alert */
.cms_alert
    &#success
        position: absolute
        top: 50px
        right: 0
        left: 0
        z-index: 999
        margin: 0 auto
        width: 300px
        text-align: center
        sf_pro_regular(18px)
        color: $white
        background-image: linear-gradient(to right, $red, $orange)
        padding: 20px
        border-radius: 5px
        box-shadow: 0px 10px 12px rgba(0,0,0,0.15)

/**
 * Pagination */
#cms_pagination
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: center
    margin-top: 60px
    user-select: none
    .next,
    .prev
        sf_pro_regular(16px)
        color: $white
        background-color: $green
        border: 1px solid $green
        padding: 8px 14px
        margin: 0 3px
        cursor: pointer
        transition: .3s ease-in-out
        &:hover,
        &:focus
            background-color: $light_green
            color: $green
            transition: .3s ease-in-out
        &.disabled
            background-color: #e2e2e2
            color: $black
            border: 1px solid #e2e2e2
            transition: .3s ease-in-out
    .number
        sf_pro_regular(16px)
        color: $green
        background-color: $light_green
        border: 1px solid $green
        padding: 8px 14px
        margin: 0 3px
        cursor: pointer
        transition: .3s ease-in-out
        &:hover,
        &:focus,
        &.active
            background-color: $green
            color: $white
            transition: .3s ease-in-out
        &.active
            pointer-events: none

/**
 * Booker **/
.booker_wrapper
    display: flex
    flex-flow: row wrap
    align-items: flex-start
    justify-content: space-between
    margin-bottom: 60px
    .booker_classes
        flex: 0 0 25%
        background-color: $white
        .header_wrapper
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            padding: 25px 15px 22px
            border-bottom: 1px solid $border
            .booker_header
                position: relative
                display: flex
                flex-flow: row wrap
                align-items: center
                .booker_title
                    sf_pro_bold(20px)
                    color: $black
                    margin: 0 15px
                .booker_prev
                    transform: rotate(180deg)
                .booker_next,
                .booker_prev
                    width: 28px
                    height: 28px
                    svg
                        .arrow_1
                            fill: $white
                            stroke: $border
                            transition: .3s ease-in-out
                        .arrow_2,
                        .arrow_4
                            fill: none
                        .arrow_2
                            stroke: $green
                            stroke-width: 1.5px
                        .arrow_3
                            stroke: none
                        &:hover
                            .arrow_1
                                fill: $border
                                transition: .3s ease-in-out
        .content_wrapper
            .class_accordion
                &.toggled
                    .accordion_header
                        &::before
                            transform: translateY(-50%) rotate(-180deg)
                            transition: .3s ease-in-out
                    .accordion_content
                        opacity: 1
                        overflow: initial
                        visibility: visible
                        transition: .3s ease-in-out
                .accordion_header
                    position: relative
                    padding: 20px
                    sf_pro_semibold(20px)
                    color: $black
                    cursor: pointer
                    &::before
                        content: url(/icons/custom-select-icon.svg)
                        position: absolute
                        top: 50%
                        right: calc(0% + 20px)
                        transform: translateY(-50%) rotate(0deg)
                        transition: .3s ease-in-out
                .accordion_content
                    height: 0
                    opacity: 0
                    overflow: hidden
                    visibility: hidden
                    transition: .3s ease-in-out
                    .no_class
                        sf_pro_regular(16px)
                        color: $black
                        padding: 20px 35px
                        text-align: center
                    .class_content
                        display: block
                        padding: 20px
                        text-decoration: none
                        box-shadow: 0px 0px 15px transparent
                        border-left: 2px solid transparent
                        border-bottom: 1px solid $border
                        transition: .3s ease-in-out
                        .class_title
                            display: flex
                            flex-flow: row wrap
                            align-items: center
                            justify-content: space-between
                            span
                                sf_pro_medium(16px)
                                color: $black
                                transition: .3s ease-in-out
                            .class_status
                                sf_pro_regular(14px)
                                color: $black
                                padding: 5px 15px
                                border: 1px solid $border
                                border-radius: 20px
                                &.full
                                    color: $red
                                    border: 1px solid $red
                        .class_text
                            sf_pro_regular(15px)
                            color: $border_alt2
                            margin-top: 5px
                            transition: .3s ease-in-out
                            &.alternate
                                display: flex
                                flex-flow: row wrap
                                align-items: center
                                justify-content: space-between
                        &:last-child
                            border-bottom: 1px solid transparent
                        &:hover,
                        &:focus
                            border-left: 2px solid $green
                            box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.1)
                            transition: .3s ease-in-out
                            .class_title
                                span
                                    color: $green
                                    transition: .3s ease-in-out
                            .class_text
                                color: $black
                                transition: .3s ease-in-out

    .booker_content
        flex: 0 0 calc(75% - 20px)
        .booker_seats
            display: block
            position: relative
            width: 100%
            overflow: hidden
            margin-bottom: 20px
            background-color: $white
            .seat_controls
                display: flex
                flex-flow: row wrap
                align-items: center
                border-bottom: 1px solid $border
                button
                    position: relative
                    padding: 0
                    width: 40px
                    height: 40px
                    background-color: transparent
                    border: none
                    box-shadow: none
                    outline: none
                    font-size: 0
                    &.margin
                        margin-left: 5px
                    &::before,
                    &::after
                        content: ''
                        position: absolute
                        background-position: center center
                        background-repeat: no-repeat
                        background-size: contain
                        width: 100%
                        height: 100%
                        top: 50%
                        left: 50%
                        transform: translate(-50%, -50%)
                        opacity: 1
                        visibility: visible
                        transition: .3s ease-in-out
                    &::after
                        opacity: 0
                        visibility: hidden
                        transition: .3s ease-in-out
                    &#zoom_in
                        &::before
                            background-image: url('/icons/zoom-in.svg')
                        &::after
                            background-image: url('/icons/zoom-in-selected.svg')
                    &#zoom_out
                        &::before
                            background-image: url('/icons/zoom-out.svg')
                        &::after
                            background-image: url('/icons/zoom-out-selected.svg')
                    &#reset
                        &::before
                            background-image: url('/icons/full-screen.svg')
                        &::after
                            background-image: url('/icons/full-screen-selected.svg')
                    &#reload
                        &::before
                            background-image: url('/icons/reload.svg')
                        &::after
                            background-image: url('/icons/reload-selected.svg')
                    &:hover
                        &::before
                            opacity: 0
                            visibility: hidden
                            transition: .3s ease-in-out
                        &::after
                            opacity: 1
                            visibility: visible
                            transition: .3s ease-in-out
                .left_side
                    padding: 20px
                    flex: 1 0 50%
                    display: flex
                    flex-flow: row wrap
                    align-items: center
                    justify-content: space-between
                    border-right: 1px solid $border
                    .class_options
                        flex: 0 0 auto
                    .booker_label
                        sf_pro_regular(18px)
                        color: $gray_alternate
                        text-align: center
                        flex: 0 0 50%
                    .controls
                        flex: 0 0 auto
                .right_side
                    padding: 20px
                    flex: 0 0 auto
                    text-align: center
            .vue-pan-zoom-item,
            .vue-pan-zoom-scene
                height: 450px
                outline: none
                overflow: hidden
            .vue-pan-zoom-scene
                .plan_wrapper
                    margin: 30px
                    height: inherit
                    position: relative
                    .no_results
                        text-align: center
                        sf_pro_medium(50px)
                        color: $gray_alternate
                        text-align: center
                        padding-top: 100px
                    .seat_boxes
                        position: absolute
                        display: flex
                        flex-flow: row wrap
                        align-items: center
                        .seat_position
                            position: relative
                            border-radius: 4px
                            text-align: center
                            cursor: pointer
                            margin: 15px
                            padding: 40px 20px
                            background-color: $white
                            border: 1.5px solid $black
                            .seat_number
                                position: absolute
                                top: 50%
                                left: 0
                                right: 0
                                text-align: center
                                transform: translateY(-50%)
                                sf_pro_bold(24px)
                                color: rgba(132, 143, 159, 0.5)
                        &.layout_1
                            &.bottom
                                bottom: calc(0% + 170px)
                            &.bottom_alt
                                bottom: calc(0% + 50px)
                                left: calc(0% - 65px)
                            &.bottom_alt_2
                                bottom: calc(0% - 70px)
                        &.layout_2
                            &.bottom
                                bottom: calc(0% + 170px)
                            &.bottom_alt
                                bottom: calc(0% + 50px)
                                left: calc(0% - 65px)
                        &.layout_3
                            &.bottom
                                bottom: calc(75%)
                            &.bottom_alt
                                bottom: calc(75% - 115px)
                                left: calc(0% - 65px)
                            &.bottom_alt_2
                                bottom: calc(75% - 230px)
                                justify-content: center
                        &.left
                            align-items: flex-start
                            top: calc(0% - 180px)
                            left: calc(0% - 60px)
                            .seat_position
                                flex: 0 0 calc(50% - 52.5px)
                                &:nth-child(2)
                                    top: calc(0px + 60px)
                                &:last-child
                                    top: calc(0px + 60px)
                        &.right
                            align-items: flex-start
                            top: calc(0% - 180px)
                            right: calc(0% - 45px)
                            .seat_position
                                flex: 0 0 calc(50% - 52.5px)
                                &:first-child
                                    top: calc(0px + 60px)
                                &:nth-child(3)
                                    top: calc(0px + 60px)
                        &.bottom,
                        &.bottom_alt,
                        &.bottom_alt_2
                            width: 100%
                            .seat_position
                                flex: 0 0 calc(10% - 30px)
            .seat_legends
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: center
                padding: 20px 0
                border-top: 1px solid $border
                .legend_title
                    display: flex
                    flex-flow: row wrap
                    align-items: center
                    justify-content: space-between
                    sf_pro_regular(16px)
                    color: $black
                    span
                        margin-right: 5px
                        display: inline-block
                        width: 15px
                        height: 15px
                        border-radius: 50%
                        background-color: $black
                        border: 1px solid $black
                    &.gray
                        span
                            background-color: $border
                    &.green
                        span
                            background-color: $light_green
                    &.white
                        span
                            background-color: $white
                    &.gradient
                        span
                            background-color: $gray_alternate
                    &.margin
                        margin-left: 20px
        .booker_footer
            display: flex
            flex-flow: row wrap
            align-items: flex-start
            justify-content: space-between
            .footer_header
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: space-between
                margin: 10px 10px 0 10px
                .action_success_btn
                    width: auto
                    padding: 10px 17px
            .footer_title
                sf_pro_semibold(16px)
                color: $black
                padding: 10px 15px
            .booker_notepad
                flex: 0 0 25%
                background-color: $white
                .notepad_text
                    border-top: 1px solid $border
                    textarea
                        sf_pro_regular(16px)
                        color: $black
                        width: 100% !important
                        border: none
                        outline: none
                        padding: 15px
                        max-height: 500px
                        overflow-y: auto
            .booker_waitlist
                flex: 0 0 calc(75% - 20px)
                background-color: $white
                .cms_waitlist
                    thead
                        border-bottom: 1px solid $border
                        tr
                            display: flex
                            align-items: center
                            th
                                font-weight: normal
                                text-align: left
                                padding: 10px 20px
                                sf_pro_regular(14px)
                                color: $border_alt2
                                flex: 0 0 30%
                                &.action
                                    flex: 1 0 20%
                    tbody
                        display: block
                        height: 100%
                        max-height: 200px
                        overflow-y: auto
                        tr
                            display: flex
                            align-items: center
                            border-bottom: 1px solid $border
                            td
                                padding: 15px 20px
                                sf_pro_medium(16px)
                                color: $blue
                                flex: 0 0 30%
                                &.action
                                    flex: 1 0 20%
                                    a
                                        text-decoration: none
                                        sf_pro_medium(16px)
                                        color: $green
                                        padding: 5px 10px
                                        border: 1px solid $green
                                        border-radius: 3px
                                        background-color: transparent
                                        transition: .3s ease-in-out
                                        &.margin
                                            margin-left: 20px
