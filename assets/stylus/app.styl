@import 'fonts'

$light_green = #E2FDF9
$green = #33B09D
$green_alt = #6ED1C2
$light_blue = #E3F7FF
$blue = #00B1FF
$blue_alt = #7AD1F7
$orange = #FFA069
$red = #FC565F
$light_red = #FFE5E6
$white = #FFF
$black = #383D40
$black_alternate = #777
$black_alternate2 = #5f6466
$gray = #91939B
$gray_alternate = #ABADB9
$border = #DEDEDE
$border_alt = #EDEEF0
$border_alt2 = #9C9EAA

image_default =
    height: auto
    vertical-align: middle
    object-fit: cover

body
    ::-webkit-scrollbar-track
    	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.1)
    	background-color: #f8f8fb
    	border-radius: 10px
    ::-webkit-scrollbar
    	width: 8px
    	background-color: #f8f8fb
    ::-webkit-scrollbar-thumb
    	border-radius: 10px
    	background-color: #C9CAD3

body
    overflow-x: hidden
    &.cms
        background-color: #f8f8fb
    &.no_click
        pointer-events: none
    &.no_scroll
        overflow-y: hidden

*,
*:before,
*:after
    user-select: none
    box-sizing: border-box
    margin: 0

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
    user-select: initial

h1,
h2,
h3,
h4,
h5
    font-weight: normal

img
    height: auto
    vertical-align: middle

table
    width: 100%
    border-collapse: collapse

/**
 * Default Loader **/
.loader_wrapper
    position: fixed
    z-index: 999
    .background
        position: fixed
        top: 0
        left: 0
        width: 100%
        height: 100%
        z-index: 2
        background-color: rgba(0, 0, 0, 0.5)
    .wrapper
        display: flex
        align-items: center
        position: fixed
        top: 50%
        left: 50%
        z-index: 3
        text-align: center
        background-image: linear-gradient(to right, $red, $orange)
        box-shadow: 0px 10px 25px rgba(0,0,0,0.15)
        padding: 0 40px
        border-radius: 5px
        transform: translate(-50%, -50%)
        .loader
            display: inline-block
            position: relative
            width: 64px
            height: 64px
            div
                display: inline-block
                position: absolute
                left: 6px
                width: 13px
                background-color: $white
                animation: loader 1.2s cubic-bezier(0, 0.5, 0.5, 1) infinite
                &:nth-child(1)
                    left: 6px
                    animation-delay: -0.20s
                &:nth-child(2)
                    left: 26px
                    animation-delay: -0.10s
                &:nth-child(3)
                    left: 45px
                    animation-delay: 0
        .loader_text
            sf_pro_regular(18px)
            color: $white
            margin-left: 10px

@keyframes loader
    0% {
        top: 6px
        height: 51px
    }
    50%, 100% {
        top: 19px
        height: 26px
    }

/**
 * Default Notification **/
.notification
    position: relative
    .notification_icon
        .notif_icon_no_fill
            fill: transparent
            transition: .3s ease-in-out
        .notif_icon
            fill: transparent
            stroke: #abadb9
            stroke-miterlimit: 10
            stroke-width: 1.5px
            transition: .3s ease-in-out
        .notif_icon_alt
            fill: #abadb9
        &.active
            .notif_icon
                fill: $green
                stroke: $green
                transition: .3s ease-in-out
            .notif_icon_no_fill
                fill: $green
                transition: .3s ease-in-out
            .notif_icon_alt
                fill: $green
    &::before
        content: '‚óè'
        position: absolute
        top: calc(0px - 15px)
        right: calc(0px - 5px)
        color: $green
        font-size: 20px
        opacity: 0
        transition: .3s ease-in-out
    .notification_box_alternate
        margin-bottom: 60px
        .wrapper
            width: 100%
            max-width: 50%
            margin: 0 auto
            .notification_wrapper
                display: flex
                flex-flow: row wrap
                align-items: flex-start
                text-decoration: none
                padding: 30px 60px
                border-radius: 0px
                background-color: $white
                border-bottom: 1px solid $border_alt
                transition: .3s ease-in-out
                span
                    color: $gray_alternate
                    transition: .3s ease-in-out
                .notification_desc
                    flex: 1 0 50%
                    padding-left: 10px
                    .notification_title
                        sf_pro_medium(15px)
                        color: $black
                        padding-bottom: 10px
                    .notification_time
                        sf_pro_regular(14px)
                        color: $gray_alternate
                &:hover
                    background-color: $light_green
                    transition: .3s ease-in-out
                    &:first-child
                        border-radius: 10px 10px 0 0
                    span
                        color: $green
                        transition: .3s ease-in-out
                &:last-child
                    border-bottom: none
        .notification_action
            display: block
            width: 100%
            text-align: center
            sf_pro_medium(16px)
            color: $blue
            text-decoration: none
            padding: 10px 0
            border-radius: 0 0 10px 10px
            background-color: transparent
            border-top: 1px solid $border_alt
            transition: .3s ease-in-out
            &:hover
                background-color: $light_blue
                transition: .3s ease-in-out
    .notification_box
        position: absolute
        right: 0
        width: 285px
        top: calc(0px + 100px)
        opacity: 0
        visibility: hidden
        background-color: $white
        border-radius: 10px
        box-shadow: 10px 10px 30px rgba(0, 0, 0, 0.1)
        transition: all .2s ease-in-out
        &.active
            top: calc(0px + 50px)
            opacity: 1
            visibility: visible
            transition: all .2s ease-in-out
        .wrapper
            height: 560px
            overflow-y: scroll
            .notification_wrapper
                display: flex
                flex-flow: row wrap
                align-items: flex-start
                justify-content: space-between
                text-decoration: none
                padding: 16px 20px
                border-radius: 0px
                background-color: transparent
                border-bottom: 1px solid $border_alt
                transition: .3s ease-in-out
                span
                    color: $gray_alternate
                    transition: .3s ease-in-out
                .notification_desc
                    flex: 0.9 0 50%
                    .notification_title
                        sf_pro_medium(15px)
                        color: $black
                        padding-bottom: 10px
                    .notification_time
                        sf_pro_regular(14px)
                        color: $gray_alternate
                &:hover
                    background-color: $light_green
                    transition: .3s ease-in-out
                    &:first-child
                        border-radius: 10px 10px 0 0
                    span
                        color: $green
                        transition: .3s ease-in-out
                &:last-child
                    border-bottom: none
        .notification_action
            display: block
            width: 100%
            text-align: center
            sf_pro_medium(16px)
            color: $blue
            text-decoration: none
            padding: 10px 0
            border-radius: 0 0 10px 10px
            background-color: transparent
            border-top: 1px solid $border_alt
            transition: .3s ease-in-out
            &:hover
                background-color: $light_blue
                transition: .3s ease-in-out
    &.active
        &::before
            opacity: 1
            animation: blink 1s infinite alternate
            transition: .3s ease-in-out

/**
 * Default Buttons **/
