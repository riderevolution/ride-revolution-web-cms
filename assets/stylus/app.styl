    top: 0
    left: 0
    z-index: 4
    width: 100%
    height: 100%
    &.alternate
        z-index: 5
    .background
        position: fixed
        z-index: 3
        top: 0
        left: 0
        width: 100%
        height: 100%
        background-color: rgba(0, 0, 0, 0.5)
