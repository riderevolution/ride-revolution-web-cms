    width: 225px
    sf_pro_medium(16px)
    text-decoration: none
    outline: none
    color: $green
    padding: 17px
    background-color: $light_green
    border: 1px solid $light_green
    border-radius: 3px
    position: relative
    text-align: center
    cursor: pointer
    user-select: none
    transition: .3s ease-in-out
    &.confirm
        width: 130px
    &.margin
        margin-left: 20px
    &.alternate
        background-color: $green
        border: 1px solid $green
        color: $white
        &:hover
            color: $green
            background-color: $light_green
            border: 1px solid $light_green
            transition: .3s ease-in-out
        &:focus
            color: $white
            background-color: $green_alt
            border: 1px solid $green_alt
            transition: .3s ease-in-out
    &:hover
        color: $white
        background-color: $green
        border: 1px solid $green
        transition: .3s ease-in-out
    &:focus
        color: $white
        background-color: $green_alt
        border: 1px solid $green_alt
        transition: .3s ease-in-out
.action_cancel_btn
    display: inline-block
    width: 225px
    sf_pro_medium(16px)
    text-decoration: none
    outline: none
    color: $red
    padding: 17px
    background-color: $light_red
    border: 1px solid $light_red
    border-radius: 3px
    position: relative
    text-align: center
    cursor: pointer
    user-select: none
    transition: .3s ease-in-out
    &.margin
        margin-left: 20px
    &.confirm
        width: 130px
    &:hover,
    &:focus
