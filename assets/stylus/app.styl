            transform: translateY(-50%) rotate(0deg)
            cursor: pointer
            transition: .3s ease-in-out
        .content_headers
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: flex-start
            background-color: $white
            border-bottom: 1px solid $border
            box-shadow: 0px 3px 10px transparent
            transition: .3s ease-in-out
            .accordion_content
                flex: 0 0 25%
                padding: 20px 30px
                sf_pro_semibold(20px)
                color: $green
                &:first-child
                    padding: 20px 30px 20px 60px
            .accordion_actions
                padding: 20px 30px
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: space-between
                &.action
                    flex: 1 0 auto
                a
                    flex: 0 0 47%
                    text-align: center
                    text-decoration: none
                    sf_pro_medium(16px)
                    padding: 10px
                    &.accordion_action_edit
                        background-color: $light_blue
                        color: $blue
                        transition: .3s ease-in-out
                        &:hover
                            background-color: $blue
                            color: $white
                            transition: .3s ease-in-out
                    &.accordion_action_cancel
                        background-color: $light_red
                        color: $red
                        transition: .3s ease-in-out
                        &:hover
                            background-color: $red
                            color: $white
                            transition: .3s ease-in-out
                    &.accordion_action_success
                        background-color: $light_green
                        color: $green
                        transition: .3s ease-in-out
                        &:hover
                            background-color: $green
                            color: $white
                            transition: .3s ease-in-out
        .accordion_table
            flex: 0 0 100%
            height: 0px
            opacity: 0
            overflow: hidden
            transition: .3s ease-in-out
            .cms_table
                margin-bottom: 0px
                margin-top: 10px
                .padding_left
                    padding: 20px 30px 20px 60px
    .no_results
        background-color: $white
        sf_pro_regular(16px)
        color: $black
