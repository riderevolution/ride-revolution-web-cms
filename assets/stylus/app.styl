            padding: 100px 0
            box-shadow: 15px 25px 40px transparent
            text-decoration: none
            transition: .3s ease-in-out
            .total_image
                .back
                    right: 0
                    margin: 0 auto
            .total_count
                sf_pro_bold(45px)
                color: $black
                margin: 15px 0
