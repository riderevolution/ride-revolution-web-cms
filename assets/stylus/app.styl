    sf_pro_medium(18px)
    text-decoration: none
    outline: none
    color: $black
    padding: 10px
    background-color: transparent
    border: 1px solid transparent
    border-radius: 3px
    position: relative
    text-align: center
    cursor: pointer
    user-select: none
    text-transform: capitalize
    transition: .3s ease-in-out
    img
        margin-right: 10px
        margin-bottom: 3px
    &:hover
        color: $green
        background-color: $light_green
        border: 1px solid $light_green
        transition: .3s ease-in-out
    &:focus
        color: $white
        background-color: $green_alt
        border: 1px solid $green_alt
        transition: .3s ease-in-out
.action_btn
    display: inline-block
    width: 225px
    sf_pro_medium(16px)
    text-decoration: none
    outline: none
    color: $blue
    padding: 17px
    background-color: $light_blue
    border: 1px solid $light_blue
    border-radius: 3px
    position: relative
    text-align: center
    cursor: pointer
    user-select: none
    transition: .3s ease-in-out
    &.white
        sf_pro_medium(14px)
        width: auto
        padding: 10px 12px
        color: $white
        background-color: transparent
        border: 1px solid $white
        transition: .3s ease-in-out
        &:hover
            border: 1px solid $white
            background-color: $white
            color: $orange
            transition: .3s ease-in-out
        &:focus
            border: 1px solid $white
            background-color: $white
            color: $red
            transition: .3s ease-in-out
    &.margin
        margin-left: 20px
    &.alternate
        margin-left: 20px
        background-color: $blue
        border: 1px solid $blue
        color: $white
        &:hover
            background-color: $light_blue
            border: 1px solid $light_blue
            color: $blue
            transition: .3s ease-in-out
        &:focus
            border: 1px solid $blue_alt
            background-color: $blue_alt
            color: $white
            transition: .3s ease-in-out
    &:hover
        border: 1px solid $blue
        background-color: $blue
        color: $white
        transition: .3s ease-in-out
    &:focus
        border: 1px solid $blue_alt
        background-color: $blue_alt
        color: $white
        transition: .3s ease-in-out
.action_highlight
    sf_pro_regular(15px)
    color: $green
    background-color: $light_green
    padding: 10px 15px
    span
        sf_pro_regular(12px)
.action_calendar_btn
    display: inline-block
    sf_pro_regular(14px)
    color: $black
    background-color: #F2F2F2
    border: 1px solid rgba(171, 173, 185, 0.25)
    padding: 10px 15px
    text-decoration: none
    outline: none
    border-radius: 3px
    transition: .3s ease-in-out
    &.margin
        margin-left: 5px
    &:hover
        background-color: rgba(171, 173, 185, 0.5)
        transition: .3s ease-in-out
.action_user_btn
    display: inline-block
    width: auto
    sf_pro_medium(16px)
    text-decoration: none
    outline: none
    color: $white
    padding: 12px 20px
    background-color: $black_alternate2
    border: 1px solid $black_alternate2
    border-radius: 3px
    position: relative
    text-align: center
    cursor: pointer
    user-select: none
    transition: .3s ease-in-out
    &.margin
        margin-left: 20px
    &:hover,
    &:focus
        color: $black_alternate2
        background-color: $white
        border: 1px solid $white
        transition: .3s ease-in-out

/**
 * Close Icon **/
.close_wrapper
    position: absolute
    top: 0
    right: 0
    left: 0
    height: 100%
    z-index: 1
    background-color: rgba(0, 0, 0, 0.5)
    margin: 0 5px
    &.alternate
        top: 50%
        left: unset
        margin: 0
        width: 25px
        height: 25px
        background-color: $red
        border-radius: 50%
        transform: translateY(-50%)
        .close_icon
            top: 50%
            left: 0
            right: 0
            margin: 0 auto
            text-align: center
            transform: translateY(-50%)
            &::before,
            &::after
                top: calc(0% + 3px)
                right: 0
                left: 0
                margin: 0 auto
                height: 13px
            &::before
                transform: rotate(45deg)
            &::after
                transform: rotate(-45deg)
    .close_icon
        position: absolute
        width: 20px
        height: 20px
        top: calc(0% + 10px)
        right: calc(0% + 10px)
        z-index: 3
        &::before
            content: ''
            position: absolute
            top: 0
            right: calc(0% + 8px)
            width: 2px
            height: 20px
            background-color: $white
            transform: rotate(45deg)
        &::after
            content: ''
            position: absolute
            top: 0
            right: calc(0% + 8px)
            width: 2px
            height: 20px
            background-color: $white
            transform: rotate(-45deg)

/**
 * Default Modal **/
.default_modal
